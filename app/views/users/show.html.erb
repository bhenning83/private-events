<h1><%= @user.name %>'s page</h1>

<h3>Your Created Events:</h3>
<ul>
  <% @user.created_events.each do |e| %>
    <li>
      <strong><%= e.name %></strong>
      <%= e.date %>
      <%= link_to 'View Event', event_path %>
    </li>
  <% end %>
</ul>

<%= link_to 'Create a new event', new_event_path %><br />
<%= link_to 'Log Out', logout_path %>
